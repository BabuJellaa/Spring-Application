<?xml version="1.0" encoding="UTF-8"?>

<beans  xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="employee1" class="com.tech.spring.SpringApplication.Employee">
		<property name="employeeId" value="1839"></property>
		<property name="employeeName" value="Babu Jella"></property>
		<property name="employeeRole" value="Software Developer"></property>
		<property name="city" value="Hyderabad"></property>
	</bean>
	 
